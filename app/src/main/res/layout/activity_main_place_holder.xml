<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/heading"
        android:layout_width="200dp"
        android:layout_height="14sp"
        android:background="@color/background"
        android:layout_marginTop="40dp"
        android:layout_marginStart="20dp"></View>
    <View
        android:id="@+id/subHeading1"
        android:layout_width="100dp"
        android:layout_height="@dimen/placeholderTextHeight"
        android:background="@color/background"
        android:layout_below="@id/heading"
        android:layout_marginTop="20dp"
        android:layout_marginStart="30dp"></View>

    <View
        android:id="@+id/cv1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/rounder_corner_grey"
        android:layout_below="@id/subHeading1"
        android:layout_marginTop="15dp"
        android:layout_marginStart="15dp"></View>
    <View
        android:id="@+id/cv2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/rounder_corner_grey"
        android:layout_below="@id/subHeading1"
        android:layout_toRightOf="@id/cv1"
        android:layout_marginTop="15dp"
        android:layout_marginStart="12dp"></View>
    <View
        android:id="@+id/cv3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/rounder_corner_grey"
        android:layout_below="@id/subHeading1"
        android:layout_toRightOf="@id/cv2"
        android:layout_marginTop="15dp"
        android:layout_marginStart="12dp"></View>
    <View
        android:id="@+id/cv4"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/rounder_corner_grey"
        android:layout_below="@id/cv1"
        android:layout_marginTop="10dp"
        android:layout_marginStart="12dp"></View>
    <View
        android:id="@+id/cv5"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/rounder_corner_grey"
        android:layout_below="@id/cv2"
        android:layout_toRightOf="@id/cv4"
        android:layout_marginTop="10dp"
        android:layout_marginStart="12dp"></View>

    <View
        android:id="@+id/cv6"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/rounder_corner_grey"
        android:layout_below="@id/cv3"
        android:layout_toRightOf="@id/cv5"
        android:layout_marginTop="10dp"
        android:layout_marginStart="12dp"></View>

    <View
        android:id="@+id/cv7"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/rounder_corner_grey"
        android:layout_below="@id/cv4"
        android:layout_marginTop="10dp"
        android:layout_marginStart="12dp"></View>

    <View
        android:id="@+id/cv8"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/rounder_corner_grey"
        android:layout_below="@id/cv5"
        android:layout_toRightOf="@id/cv7"
        android:layout_marginTop="10dp"
        android:layout_marginStart="12dp"></View>
    <View
        android:id="@+id/subHeading2"
        android:layout_width="100dp"
        android:layout_height="@dimen/placeholderTextHeight"
        android:background="@color/background"
        android:layout_below="@id/cv7"
        android:layout_marginTop="15dp"
        android:layout_marginStart="30dp"></View>

    <View
        android:id="@+id/cv9"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/rounder_corner_grey"
        android:layout_below="@id/subHeading2"
        android:layout_marginTop="15dp"
        android:layout_marginStart="12dp"></View>

    <View
        android:id="@+id/cv10"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/rounder_corner_grey"
        android:layout_below="@id/subHeading2"
        android:layout_toRightOf="@id/cv9"
        android:layout_marginTop="15dp"
        android:layout_marginStart="12dp"></View>
    <View
        android:id="@+id/cv11"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/rounder_corner_grey"
        android:layout_below="@id/subHeading2"
        android:layout_toRightOf="@id/cv10"
        android:layout_marginTop="15dp"
        android:layout_marginStart="12dp"></View>
</RelativeLayout>